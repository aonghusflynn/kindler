	<meta name="description" content="Kris Kindle allows you to play Secret Santa with your family and friends. Share the joy of giving!" />

	<meta itemprop="description" content="Kris Kindle allows you to play Secret Santa with your family and friends. Share the joy of giving!">

	<meta property="og:title" content="Kris Kindle" />
	<meta property="og:type" content="website" />
